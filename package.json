{
  "name": "hdtickets",
  "version": "2.0.0",
  "private": true,
  "description": "HD Tickets - Sports Event Ticket Monitoring Platform",
  "homepage": "https://github.com/polascin/hdTickets#readme",
  "bugs": {
    "url": "https://github.com/polascin/hdTickets/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/polascin/hdTickets.git"
  },
  "license": "ISC",
  "author": "",
  "type": "module",
  "main": "eslint.config.js",
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "scripts": {
    "dev": "vite",
    "dev:frameworks": "concurrently \"npm run dev\" \"npm run dev:react\" \"npm run dev:vue\" \"npm run dev:angular\"",
    "dev:react": "echo \"React development server integrated with Vite\"",
    "dev:vue": "echo \"Vue development server integrated with Vite\"",
    "dev:angular": "echo \"Angular development server integrated with Vite\"",
    "build": "vite build",
    "build:frameworks": "npm run build:react && npm run build:vue && npm run build:angular && npm run build",
    "build:react": "vite build --mode react",
    "build:vue": "vite build --mode vue",
    "build:angular": "vite build --mode angular",
    "preview": "vite preview",
    "lint": "npm run lint:js && npm run lint:vue && npm run lint:angular",
    "lint:js": "eslint resources/js --ext .js,.ts,.tsx",
    "lint:vue": "eslint resources/js/frameworks/vue --ext .js,.ts,.vue",
    "lint:angular": "eslint resources/js/frameworks/angular --ext .js,.ts",
    "lint:fix": "npm run lint:js -- --fix && npm run lint:vue -- --fix && npm run lint:angular -- --fix",
    "analyze": "vite build --mode analyze",
    "analyze:frameworks": "npm run analyze:react && npm run analyze:vue && npm run analyze:angular",
    "analyze:react": "vite-bundle-analyzer public/build/assets/react-*.js",
    "analyze:vue": "vite-bundle-analyzer public/build/assets/vue-*.js",
    "analyze:angular": "vite-bundle-analyzer public/build/assets/angular-*.js",
    "test": "npm run test:unit && npm run test:frameworks",
    "test:unit": "vitest",
    "test:frameworks": "npm run test:react && npm run test:vue && npm run test:angular",
    "test:react": "vitest --config vitest.config.react.ts",
    "test:vue": "vitest --config vitest.config.vue.ts",
    "test:angular": "vitest --config vitest.config.angular.ts",
    "test:coverage": "vitest --coverage",
    "test:coverage:frameworks": "npm run test:react -- --coverage && npm run test:vue -- --coverage && npm run test:angular -- --coverage",
    "format": "prettier --write resources/**/*.{js,ts,tsx,vue,css,scss}",
    "format:check": "prettier --check resources/**/*.{js,ts,tsx,vue,css,scss}",
    "clean": "rm -rf public/build",
    "clean:frameworks": "rm -rf public/build/assets/{react,vue,angular}-*",
    "type-check": "tsc --noEmit && vue-tsc --noEmit",
    "type-check:frameworks": "npm run type-check:react && npm run type-check:vue && npm run type-check:angular",
    "type-check:react": "tsc --noEmit --project tsconfig.react.json",
    "type-check:vue": "vue-tsc --noEmit --project tsconfig.vue.json",
    "type-check:angular": "tsc --noEmit --project tsconfig.angular.json",
    "e2e:install": "playwright install chromium",
    "test:e2e": "playwright test --project=chromium",
    "test:e2e:frameworks": "playwright test --project=multi-framework",
    "e2e": "start-server-and-test 'php artisan serve --host=********* --port=8000' http://*********:8000 'playwright test --project=chromium'",
    "framework:status": "node scripts/framework-status.js",
    "framework:init": "node scripts/framework-init.js"
  },
  "dependencies": {
    "@alpinejs/focus": "^3.14.1",
    "@alpinejs/persist": "^3.14.1",
    "@angular/animations": "^20.3.3",
    "@angular/common": "^20.3.3",
    "@angular/core": "^20.3.3",
    "@angular/elements": "^20.3.3",
    "@angular/forms": "^20.3.3",
    "@angular/platform-browser": "^20.3.3",
    "@angular/platform-browser-dynamic": "^20.3.3",
    "@angular/router": "^20.3.3",
    "@headlessui/react": "^2.2.9",
