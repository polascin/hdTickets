  "categories": ["sports", "entertainment", "productivity"],
  "screenshots": [
    {
      "src": "/assets/images/pwa/screenshot-mobile.png",
      "sizes": "320x640",
      "type": "image/png"
    },
    {
      "src": "/assets/images/pwa/screenshot-desktop.png",
      "sizes": "1280x720",
      "type": "image/png"
    }
  ],
  "features": [
    "real-time ticket monitoring",
    "push notifications",
    "offline access",
    "background sync"
  ],
  "prefer_related_applications": false,
  "shortcuts": [
    {
      "name": "Dashboard",
      "short_name": "Dashboard",
      "description": "View main dashboard",
      "url": "/dashboard",
      "icons": [
        {
          "src": "/assets/images/pwa/shortcut-dashboard.png",
          "sizes": "96x96"
        }
      ]
    },
    {
      "name": "Admin Scraping",
      "short_name": "Admin",
      "description": "Advanced scraping management",
      "url": "/admin/scraping",
      "icons": [
        {
          "src": "/assets/images/pwa/shortcut-admin.png",
          "sizes": "96x96"
        }
      ]
    },
    {
      "name": "Ticket Alerts",
      "short_name": "Alerts",
      "description": "View ticket alerts",
      "url": "/tickets/alerts",
      "icons": [
        {
          "src": "/assets/images/pwa/shortcut-alerts.png",
          "sizes": "96x96"
        }
      ]
    },
    {
      "name": "Scraping Monitor",
      "short_name": "Monitor",
      "description": "Monitor ticket scraping",
      "url": "/tickets/scraping",
      "icons": [
        {
          "src": "/assets/images/pwa/shortcut-monitor.png",
          "sizes": "96x96"
        }
      ]
    },
    {
      "name": "PWA Features",
      "short_name": "PWA",
      "description": "Test PWA capabilities",
      "url": "/admin/scraping?pwa=true",
      "icons": [
        {
          "src": "/assets/images/pwa/shortcut-pwa.png",
          "sizes": "96x96"
        }
      ]
    }
  ],
  "protocol_handlers": [
    {
      "protocol": "web+hdtickets",
      "url": "/admin/handle/%s"
    }
  ],
  "handle_links": "preferred",
  "launch_handler": {
    "client_mode": "navigate-new"
  },
  "edge_side_panel": {
    "preferred_width": 400
  },
  "file_handlers": [
    {
      "action": "/admin/import",
      "accept": {
        "text/csv": [".csv"],
        "application/json": [".json"]
      }
    }
  ],
  "share_target": {
    "action": "/admin/import-shared",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "files": [
        {
          "name": "data",
          "accept": ["text/csv", "application/json"]
        }
      ]
    }
  },
  "display_override": ["window-controls-overlay", "minimal-ui", "standalone"],
  "related_applications": [],
  "iarc_rating_id": "",
  "dir": "ltr",
  "lang": "en",
  "version": "1.2.0"
}
