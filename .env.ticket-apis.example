# Ticket API Configuration Example
# Copy these settings to your .env file and add your actual API keys

# Ticketmaster Discovery API
# Sign up at: https://developer.ticketmaster.com/
TICKETMASTER_ENABLED=false
TICKETMASTER_API_KEY=your_ticketmaster_api_key_here

# SeatGeek Platform API  
# Sign up at: https://platform.seatgeek.com/
SEATGEEK_ENABLED=false
SEATGEEK_CLIENT_ID=your_seatgeek_client_id_here
SEATGEEK_CLIENT_SECRET=your_seatgeek_client_secret_here

# Eventbrite API
# Sign up at: https://www.eventbrite.com/platform/
EVENTBRITE_ENABLED=false
EVENTBRITE_API_KEY=your_eventbrite_api_key_here

# StubHub Partner API (Partner Program Required)
# Apply at: https://stubhub.com/partners
STUBHUB_ENABLED=false
STUBHUB_API_KEY=your_stubhub_api_key_here
STUBHUB_APP_TOKEN=your_stubhub_app_token_here
STUBHUB_SANDBOX=true

# Bandsintown API
# Register at: https://manager.bandsintown.com/support/api
BANDSINTOWN_ENABLED=false
BANDSINTOWN_APP_ID=your_bandsintown_app_id_here
