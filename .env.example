APP_NAME="HD Tickets"
APP_ENV=production
APP_KEY=base64:YOUR_APP_KEY_HERE
APP_DEBUG=false
APP_TIMEZONE=UTC
APP_URL=https://hdtickets.local
APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

MAINTENANCE_DRIVER=file
MAINTENANCE_STORE=database

# Logging Configuration
LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=error
# Enhanced logging for PHP 8.4 compatibility
LOG_SLACK_WEBHOOK_URL=
LOG_STDERR_FORMATTER=Monolog\Formatter\JsonFormatter

# Database Configuration - MariaDB 10.4 Compatible
DB_CONNECTION=mariadb
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE=hdtickets
DB_USERNAME=hdtickets_user
DB_PASSWORD=your_secure_password
DB_CHARSET=utf8mb4
DB_COLLATION=utf8mb4_unicode_ci
DB_STRICT=true
DB_ENGINE=InnoDB

# Session Configuration\nSESSION_DRIVER=database\nSESSION_LIFETIME=60\nSESSION_ENCRYPT=true\nSESSION_PATH=/\nSESSION_DOMAIN=.hdtickets.local\nSESSION_IP_VALIDATION=false

# Cache Configuration
CACHE_STORE=redis
CACHE_PREFIX=hdtickets_cache

# Queue Configuration
QUEUE_CONNECTION=redis
QUEUE_DEFAULT=default

# Redis Configuration
REDIS_CLIENT=phpredis
REDIS_HOST=127.0.0.1
REDIS_PORT=6379
REDIS_PASSWORD=null
REDIS_DB=0

# Mail Configuration
MAIL_MAILER=log
MAIL_HOST=127.0.0.1
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_ENCRYPTION=null
MAIL_FROM_ADDRESS="noreply@hdtickets.local"
MAIL_FROM_NAME="${APP_NAME}"

# Broadcasting (Soketi WebSocket Server)
BROADCAST_CONNECTION=soketi
BROADCAST_DRIVER=soketi

# Soketi Configuration
SOKETI_APP_ID=hd-tickets-app
SOKETI_APP_KEY=hd-tickets-key
SOKETI_APP_SECRET=hd-tickets-secret
SOKETI_HOST=127.0.0.1
SOKETI_PORT=6001
SOKETI_SCHEME=http

# Pusher Configuration (used by Soketi)
PUSHER_APP_ID="${SOKETI_APP_ID}"
PUSHER_APP_KEY="${SOKETI_APP_KEY}"
PUSHER_APP_SECRET="${SOKETI_APP_SECRET}"
PUSHER_HOST="${SOKETI_HOST}"
PUSHER_PORT="${SOKETI_PORT}"
PUSHER_SCHEME="${SOKETI_SCHEME}"
PUSHER_APP_CLUSTER=mt1

# Vite Frontend Build
VITE_APP_NAME="${APP_NAME}"
VITE_PUSHER_APP_KEY="${PUSHER_APP_KEY}"
VITE_PUSHER_HOST="${PUSHER_HOST}"
VITE_PUSHER_PORT="${PUSHER_PORT}"
VITE_PUSHER_SCHEME="${PUSHER_SCHEME}"
VITE_PUSHER_APP_CLUSTER="${PUSHER_APP_CLUSTER}"

# Third-party APIs
TICKET_PLATFORM_API_KEY=your_api_key
TICKETMASTER_API_KEY=your_ticketmaster_api_key
STUBHUB_API_KEY=your_stubhub_api_key
VIAGOGO_API_KEY=your_viagogo_api_key
TICKPICK_API_KEY=your_tickpick_api_key

# Security
BCRYPT_ROUNDS=12

# Authentication Configuration
AUTH_ENHANCED_LOGIN=true
AUTH_COMPREHENSIVE_LOGIN=true
AUTH_MAX_FAILED_ATTEMPTS=5
AUTH_LOCKOUT_DURATION=900
AUTH_DEVICE_FINGERPRINTING=true
AUTH_GEOLOCATION_TRACKING=true
AUTH_PROGRESSIVE_VALIDATION=true
AUTH_BIOMETRIC_AUTH=true
AUTH_PASSWORD_STRENGTH_METER=true
AUTH_SESSION_WARNINGS=true
AUTH_REGISTRATION_TWO_FACTOR_PROMPT=true

# Laravel Sanctum Configuration
SANCTUM_STATEFUL_DOMAINS=localhost,127.0.0.1,hdtickets.local
SANCTUM_TOKEN_EXPIRATION=525600
SANCTUM_REFRESH_THRESHOLD=43200
SANCTUM_RATE_LIMITING=true
SANCTUM_MAX_ATTEMPTS=60
SANCTUM_DECAY_MINUTES=1
SANCTUM_SECURE_COOKIES=false
SANCTUM_SAME_SITE=lax

# Telescope (Laravel Debugging)
TELESCOPE_ENABLED=false

# Laravel Horizon (Queue Management)
HORIZON_DOMAIN=hdtickets.local
HORIZON_PREFIX=horizon:
HORIZON_REDIS_CONNECTION=horizon

# Laravel Passport (OAuth2)
PASSPORT_PRIVATE_KEY=
PASSPORT_PUBLIC_KEY=

# Google OAuth Configuration
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret
GOOGLE_REDIRECT_URL=${APP_URL}/auth/google/callback

# Google reCAPTCHA Configuration
RECAPTCHA_SITE_KEY=your_recaptcha_site_key
RECAPTCHA_SECRET_KEY=your_recaptcha_secret_key
RECAPTCHA_ENABLED=true
RECAPTCHA_VERSION=v2
RECAPTCHA_THRESHOLD=0.5

# File Systems
FILESYSTEM_DISK=local
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

# Scout Search (if needed)
SCOUT_DRIVER=meilisearch
MEILISEARCH_HOST=http://127.0.0.1:7700

# Rate Limiting
THROTTLE_REQUESTS=60
THROTTLE_MINUTES=1

# Notification Channels
SLACK_WEBHOOK_URL=
DISCORD_WEBHOOK_URL=
TELEGRAM_BOT_TOKEN=
TELEGRAM_CHAT_ID=

# Performance Settings
OPCACHE_ENABLE=1
CACHE_DRIVER=redis

# User Management & Subscription Settings
SUBSCRIPTION_FREE_ACCESS_DAYS=7
SUBSCRIPTION_DEFAULT_MONTHLY_FEE=29.99
SUBSCRIPTION_DEFAULT_TICKET_LIMIT=100
SUBSCRIPTION_AGENT_UNLIMITED_TICKETS=true

# Stripe Payment Configuration
STRIPE_KEY=pk_test_your_stripe_publishable_key
STRIPE_SECRET=sk_test_your_stripe_secret_key
STRIPE_WEBHOOK_SECRET=whsec_your_webhook_secret

# PayPal Payment Configuration
PAYPAL_MODE=sandbox
PAYPAL_SANDBOX_CLIENT_ID=your_paypal_sandbox_client_id
PAYPAL_SANDBOX_CLIENT_SECRET=your_paypal_sandbox_client_secret
PAYPAL_LIVE_CLIENT_ID=your_paypal_live_client_id
PAYPAL_LIVE_CLIENT_SECRET=your_paypal_live_client_secret
PAYPAL_WEBHOOK_ID=your_paypal_webhook_id
PAYPAL_RECEIVER_EMAIL=walter.csoelle@gmail.com

# Twilio SMS Configuration (for phone verification)
TWILIO_SID=your_twilio_account_sid
TWILIO_TOKEN=your_twilio_auth_token
TWILIO_FROM=+1234567890

# Legal Documents Configuration
LEGAL_DOCUMENTS_VERSION=1.0
LEGAL_DOCUMENTS_EFFECTIVE_DATE=2025-01-01

# Service Disclaimers
SERVICE_PROVIDED_AS_IS=true
SERVICE_NO_WARRANTY=true
SERVICE_NO_MONEY_BACK_GUARANTEE=true

# SEO & Analytics Configuration
GOOGLE_ANALYTICS_ID=G-XXXXXXXXXX
GOOGLE_SEARCH_CONSOLE_VERIFICATION=your_gsc_verification_code
BING_WEBMASTER_VERIFICATION=your_bing_verification_code
YANDEX_WEBMASTER_VERIFICATION=your_yandex_verification_code
FACEBOOK_PIXEL_ID=your_facebook_pixel_id
HOTJAR_ID=your_hotjar_tracking_id
MICROSOFT_CLARITY_ID=your_microsoft_clarity_id

# SEO Settings
SEO_SITEMAP_CACHE_MINUTES=60
SEO_PAGE_CACHE_MINUTES=30
SEO_ROBOTS_CRAWL_DELAY=5
SEO_ENABLE_STRUCTURED_DATA=true
SEO_ENABLE_BREADCRUMBS=true

# IMAP Email Monitoring Configuration
IMAP_DEFAULT_CONNECTION=gmail

# Gmail IMAP Settings
IMAP_GMAIL_HOST=imap.gmail.com
IMAP_GMAIL_PORT=993
IMAP_GMAIL_PROTOCOL=imap
IMAP_GMAIL_ENCRYPTION=ssl
IMAP_GMAIL_VALIDATE_CERT=true
IMAP_GMAIL_USERNAME=your_gmail_email@gmail.com
IMAP_GMAIL_PASSWORD=your_app_password
IMAP_GMAIL_TIMEOUT=60
IMAP_GMAIL_RETRY_COUNT=3
IMAP_GMAIL_RETRY_DELAY=5

# Outlook IMAP Settings
IMAP_OUTLOOK_HOST=outlook.office365.com
IMAP_OUTLOOK_PORT=993
IMAP_OUTLOOK_PROTOCOL=imap
IMAP_OUTLOOK_ENCRYPTION=ssl
IMAP_OUTLOOK_VALIDATE_CERT=true
IMAP_OUTLOOK_USERNAME=your_email@outlook.com
IMAP_OUTLOOK_PASSWORD=your_password
IMAP_OUTLOOK_TIMEOUT=60
IMAP_OUTLOOK_RETRY_COUNT=3
IMAP_OUTLOOK_RETRY_DELAY=5

# Yahoo IMAP Settings
IMAP_YAHOO_HOST=imap.mail.yahoo.com
IMAP_YAHOO_PORT=993
IMAP_YAHOO_PROTOCOL=imap
IMAP_YAHOO_ENCRYPTION=ssl
IMAP_YAHOO_VALIDATE_CERT=true
IMAP_YAHOO_USERNAME=your_email@yahoo.com
IMAP_YAHOO_PASSWORD=your_app_password
IMAP_YAHOO_TIMEOUT=60
IMAP_YAHOO_RETRY_COUNT=3
IMAP_YAHOO_RETRY_DELAY=5

# Custom IMAP Settings
IMAP_CUSTOM_HOST=
IMAP_CUSTOM_PORT=993
IMAP_CUSTOM_PROTOCOL=imap
IMAP_CUSTOM_ENCRYPTION=ssl
IMAP_CUSTOM_VALIDATE_CERT=true
IMAP_CUSTOM_USERNAME=
IMAP_CUSTOM_PASSWORD=
IMAP_CUSTOM_TIMEOUT=60
IMAP_CUSTOM_RETRY_COUNT=3
IMAP_CUSTOM_RETRY_DELAY=5

# IMAP Monitoring Settings
IMAP_DEFAULT_MAILBOX=INBOX
IMAP_BATCH_SIZE=50
IMAP_MAX_AGE_DAYS=7
IMAP_MARK_AS_READ=true
IMAP_MOVE_PROCESSED=false
IMAP_PROCESSED_FOLDER=INBOX.Processed
IMAP_AUTO_ARCHIVE=false
IMAP_ARCHIVE_FOLDER=INBOX.Archive
IMAP_ARCHIVE_AFTER_DAYS=30

# IMAP Security Settings
IMAP_VERIFY_PEER=true
IMAP_VERIFY_PEER_NAME=true
IMAP_ALLOW_SELF_SIGNED=false
IMAP_CONNECTION_TIMEOUT=30
IMAP_READ_TIMEOUT=60
IMAP_MAX_EMAIL_SIZE_MB=10
IMAP_RATE_LIMITING=true
IMAP_RATE_LIMIT_PER_MINUTE=60

# IMAP Logging Settings
IMAP_LOGGING_ENABLED=true
IMAP_LOGGING_LEVEL=info
IMAP_LOGGING_CHANNEL=imap
IMAP_LOG_PROCESSED=true
IMAP_LOG_FAILURES=true
IMAP_LOG_CONNECTIONS=true
IMAP_MAX_LOG_SIZE_MB=100

# IMAP Queue Settings
IMAP_QUEUE_ENABLED=true
IMAP_QUEUE_CONNECTION=redis
IMAP_QUEUE_NAME=email-processing
IMAP_QUEUE_RETRY_ATTEMPTS=3
IMAP_QUEUE_RETRY_DELAY=60

# IMAP Cache Settings
IMAP_CACHE_ENABLED=true
IMAP_CACHE_DRIVER=redis
IMAP_CACHE_PREFIX=hdtickets_imap
IMAP_CONNECTION_CACHE_TTL=30
IMAP_EMAIL_CACHE_TTL=60
IMAP_PROCESSED_CACHE_TTL=24

# PayPal Configuration
PAYPAL_MODE=sandbox
PAYPAL_SANDBOX_CLIENT_ID=
PAYPAL_SANDBOX_CLIENT_SECRET=
PAYPAL_LIVE_CLIENT_ID=
PAYPAL_LIVE_CLIENT_SECRET=
PAYPAL_WEBHOOK_ID=
PAYPAL_RECEIVER_EMAIL=walter.csoelle@gmail.com
PAYPAL_MONTHLY_PLAN_ID=
