{
  "name": "sports-ticket-monitor/app",
  "description": "Sports Event Ticket Availability Monitoring System - A comprehensive platform for monitoring ticket availability across multiple sports event platforms",
  "license": "proprietary",
  "type": "project",
  "keywords": [
    "sports",
    "tickets",
    "monitoring",
    "alerts",
    "events",
    "scraping"
  ],
  "authors": [
    {
      "name": "Lubomir Polascin (Ľubomír Polaščín) aka Walter Csoelle",
      "role": "Developer & System Architect",
      "email": "info@example.com",
      "homepage": "https://github.com/walter-csoelle"
    }
  ],
  "require": {
    "php": "^8.3",
    "laravel/framework": "^11.0",
    "laravel/tinker": "^2.9",
    "laravel/sanctum": "^4.0",
    "laravel/passport": "^12.0|^13.0",
    "guzzlehttp/guzzle": "^7.0",
    "symfony/dom-crawler": "^7.0",
    "symfony/css-selector": "^7.0",
    "barryvdh/laravel-dompdf": "^3.0",
    "spatie/laravel-activitylog": "^4.8",
    "twilio/sdk": "^8.7",
    "pusher/pusher-php-server": "^7.2",
    "predis/predis": "^3.1",
    "roach-php/laravel": "^3.0",
    "spatie/browsershot": "^5.0.5",
    "pragmarx/google2fa": "^8.0",
    "pragmarx/google2fa-qrcode": "^3.0",
    "stripe/stripe-php": "^17.4",
    "laravel/slack-notification-channel": "^3.6",
    "paypal/paypal-server-sdk": "^1.1",
    "laravel/horizon": "^5.33",
    "intervention/image": "^3.0",
    "jenssegers/agent": "^2.6"
  },
  "require-dev": {
    "laravel/breeze": "^2.3",
    "phpunit/phpunit": "^11.5",
    "mockery/mockery": "^1.6",
    "fakerphp/faker": "^1.24",
    "laravel/pint": "^1.0",
    "nunomaduro/collision": "^8.4",
    "larastan/larastan": "^3.7",
    "spatie/laravel-ignition": "^2.8",
    "symfony/var-dumper": "^7.3",
    "laravel/telescope": "^5.10",
    "friendsofphp/php-cs-fixer": "^3.64",
    "phpstan/phpstan": "^2.1",
    "phpmd/phpmd": "^2.15",
    "squizlabs/php_codesniffer": "^3.10",
    "phpmetrics/phpmetrics": "^2.8",
    "rector/rector": "^2.0",
    "driftingly/rector-laravel": "^2.0",
    "phpoffice/phpspreadsheet": "^1.29"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    },
    "files": [
      "app/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-update-cmd": [
      "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi",
      "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
      "@php artisan migrate --graceful --ansi"
    ],
    "code-style": [
      "./vendor/bin/php-cs-fixer fix --config=.php-cs-fixer.php --verbose --show-progress=dots"
    ],
    "code-style-check": [
      "./vendor/bin/php-cs-fixer fix --config=.php-cs-fixer.php --dry-run --verbose --show-progress=dots"
    ],
    "static-analysis": [
      "./vendor/bin/phpstan analyse --configuration=phpstan.neon"
    ],
    "static-analysis-highmem": [
      "php -d memory_limit=512M ./vendor/bin/phpstan analyse --configuration=phpstan.neon"
    ],
    "static-analysis-refresh-baseline": [
      "php -d memory_limit=512M ./vendor/bin/phpstan analyse --configuration=phpstan.neon --generate-baseline"
    ],
    "code-quality": [
      "@code-style-check",
      "@static-analysis"
    ],
    "fix-code": [
      "@code-style"
    ],
    "quality-metrics": [
      "./vendor/bin/phpmetrics --report-html=storage/quality/metrics app/"
    ],
    "test-coverage": [
      "./vendor/bin/phpunit --coverage-html storage/quality/coverage"
    ],
    "full-quality-check": [
      "@code-quality",
      "@test",
      "@quality-metrics"
    ]
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "config": {
    "allow-plugins": {
      "php-http/discovery": true
    }
  }
}
